{
	"name": "Python 3",
	"forwardPorts": [
	  8000
	],
	"build": {
	  "dockerfile": "Dockerfile",
	  "context": "..",
	  "args": {
		"VARIANT": "3.11-bullseye"
	  }
	},
	"customizations": {
	  "vscode": {
		"settings": {
		  "python.defaultInterpreterPath": "/home/vscode/venv/bin/python"
		},
		"extensions": [
		  "ms-python.python",
		  "ms-python.vscode-pylance",
		  "GitHub.copilot-chat"
		]
	  }
	},
	"remoteUser": "vscode"
  }
